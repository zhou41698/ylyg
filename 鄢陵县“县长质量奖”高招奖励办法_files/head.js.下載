function TopLogin() {
    var userName = $("#TopUserName").val();
    var pwd = $("#TopPassWord").val();
    var choicepage = $("#ChoicePage").val();
    var isPass = validateLogin(userName, pwd);
    if (isPass) {
        var url = "/Login.GetRSA.data";
        jQuery.getJSON(url,
            { t: new Date() },
            function (json) {
                setMaxDigits(129);
                var key = new RSAKeyPair(json.Exponent, "", json.Modulus);
                var pwdRtn = encryptedString(key, pwd);
                var info = {};
                info.UserName = userName;
                info.PassWord = pwdRtn;
                info.IsSave = false;
                info.host = window.location.host;

                $("#LoginBtn").attr("disabled", "disabled");
                $.post("/Login.DoNewLogin.data", info, function (data) {

                    if (data.R == "ok" || data.R == "2") {
                        if (data.R == "2") {
                            $.alert(data.Text);
                        }

                        window.CurrentUser = data.User;
                        if (choicepage != null && choicepage != "") {
                            window.location = choicepage + "?UnitID=" + window.UnitID;
                        } else {
                            if (data.SSO != "") {
                                var img = new Image();
                                img.onerror = winRef;
                                img.onsuccess = winRef;
                                img.src = data.SSO;
                            } else {
                                winRef();
                            }
                        }

                        if (data.IsShowWin) {
                            //UserDefectWin();
                        }
                    } else {
                        if (data.R == "0") {
                            $.alert(data.Text);
                        }
                        $("#LoginBtn").removeAttr("disabled");
                    }
                }, "json");
            });


    }
}


function PunishListWin(unitID) {
    $.post("/Login.GetDetail.data", { "UnitID": unitID }, function (data) {
        if (data.count > 0) {
            var html = [];
            html.push("<div id=\"xiangq\">");
            html.push("您单位目前的积分为" + data.Point + "分，信用等级为" + data.UnitLevel + "级。");
            html.push("<table cellspacing=\"0\" cellpadding=\"0\" class=\"itemList\">");
            html.push("<thead>");
            html.push("  <tr>");
            html.push("    <th width=\"100px\">时间</th>");
            html.push("    <th width=\"150px\">原因</th>");
            html.push("    <th width=\"80px\">单位积分</th>");
            html.push("    <th width=\"80px\">信用等级</th>");
            html.push("  </tr>");
            html.push("</thead>");
            html.push("<tbody>");
            for (var i = 0; i < data.PunishDetail.length; i++) {
                html.push("  <tr>");
                var detail = data.PunishDetail[i];
                html.push("    <td>" + detail.AddTime + "</td>");
                html.push("    <td style=\"text-align:left;width:375px;\">" + detail.BadContent + "</td>");
                html.push("    <td>" + detail.ReducePoint + "</td>");
                html.push("    <td>" + detail.ReputationLevel + "</td>");

                html.push("  <tr>");
            }
            html.push("</tbody>");
            html.push("</table>");
            html.push("</div>");

            $.dialog({
                title: "查看详细",
                content: html.join(""),
                lock: true,
                cancel: true,
                cancelVal: "确定",
            });
        }

    }, "json");
}

function Login() {
    var userName = $("#LoginUserName").val();
    var pwd = $("#PassWord").val();
    var isSave = true;
    if ($("#IsSave").length > 0)
        isSave = $("#IsSave")[0].checked;
    var isPass = validateLogin(userName, pwd);
    if (isPass) {

        var url = "/Login.GetRSA.data";

        jQuery.getJSON(url,
            { t: new Date() },
            function (json) {
                setMaxDigits(129);
                var key = new RSAKeyPair(json.Exponent, "", json.Modulus);
                var pwdRtn = encryptedString(key, pwd);
                var nameRtn = encryptedString(key, userName);


                var info = {};
                info.UserName = nameRtn;
                info.PassWord = pwdRtn;
                info.IsSave = isSave;
                info.host = window.location.host;

                $("#LoginBtn").attr("disabled", "disabled");
                $.post("/Login.DoLogin.data", info, function (data) {

                    if (data.R == "ok" || data.R == "2") {
                        if (data.R == "2") {
                            $.alert(data.Text);
                        }

                        window.CurrentUser = data.User;
                        if (data.SSO != "") {
                            var img = new Image();
                            img.onerror = winRef;
                            img.onsuccess = winRef;
                            img.src = data.SSO;
                        } else {
                            winRef();
                        }


                        if (data.IsShowWin) {
                            //UserDefectWin();
                        }

                    } else {
                        if (data.R == "0") {
                            $.alert(data.Text);
                        }
                        $("#LoginBtn").removeAttr("disabled");
                    }
                }, "json");
            });


        
    }
}
function alreadLogin(isLogin) {
    if (isLogin) {
        $.post("/Login.AlreadyLogin.data", { host: window.location.host }, function (data) {
            if (data.R != "ok") {
                InitLogin();
            }
            else {
                window.CurrentUser = data.User;
                if (data.SSO != "") {
                    var img = new Image();
                    img.onerror = winRef;
                    img.onsuccess = winRef;
                    img.src = data.SSO;
                } else {
                    winRef();
                }

                if (data.IsShowWin) {
                    //UserDefectWin();
                }
            }
        }, "json");
    }
}

function validateLogin(userName, pwd) {
    var numReg = new RegExp("^[0-9]{3,9}");
    var usertext = (zGZNL ? "注册号" : "网校号");
    var emailReg = new RegExp("^\\w+((-\\w+)|(\\.\\w+))*\\@[A-Za-z0-9]+((\\.|-)[A-Za-z0-9]+)*\\.[A-Za-z0-9]+$");
    if (userName == "" || userName == (usertext + "/邮箱/手机号")) {
        alert("请输入" + usertext + "或者Email");
        $("#LoginUserName").focus();
        return false;
    }
    if (!numReg.test(userName) && !emailReg.test(userName)) {
        alert("请输入正确的" + usertext + "或者邮箱或手机号");
        $("#LoginUserName").focus();
        return false;

    }
    if (pwd == "" || pwd.length == 0) {
        alert("请输入密码");
        $("#PassWord").focus();
        return false;
    }
    return true;

}


function LoginOut() {
    window.CurrentUser = null;
    $.post("/Login.LoginOut.data", { host: window.location.host }, function (sso) {
        if (sso != "") {
            var img = new Image();
            img.onerror = winRef;
            img.onsuccess = winRef;
            img.src = sso;
        } else {
            winRef();
        }
    });
}


function InitMenu() {
    var menu = $("ul.head_menu");
    menu.find("a[href='" + location.pathname + "']").parents("li.m").addClass("active");

    var t = menu.attr("t");
    var childMenu = menu.find(">li");
    var minX = 232;
    var widthmax = 960;
    var childwidth = menu.find("li li").width();
    if (t == "2") {
        childMenu.each(function () {

            var width = $(this).find("li").length * childwidth;
            if (width > widthmax) width = widthmax;
            var maxX = widthmax - width + minX;
            var x = $(this).offset().left - width / 2 + childwidth / 2;
            if (x < minX) x = minX;
            if (x > maxX) x = maxX;
            $(this).find("ul").css({ width: width, left: x });

        });
    }
    if (t == "3") {
        childMenu.each(function () {
            if ($(this).find("li").length > 15) {
                var width = (parseInt(($(this).find("li").length - 1) / 15) + 1) * childwidth;
                if (width > widthmax) width = widthmax;
                $(this).find("ul").css({ width: width });
            }

        });
    }


    childMenu.bind("mouseover", function () {
        childMenu.removeClass("hover");
        $(this).addClass("hover");
        //$(this).find("ul").show();
        $(this).find(".menu-ul>li").hover(function () {
            $(this).addClass("on");
        }, function () {
            $(this).removeClass("on");
        });
    });


    childMenu.bind("mouseout", function () {
        $(this).removeClass("hover");
        //$(this).find("ul").hide();
    });

    if ($(".menu-ul li").find("ul").html() != "") {
        $(".menu-ul li").find("div.menu-ul-sub").siblings("a").append("<span class='sub'></span>");
    }

}

function winRef() {
    if (typeof (window.needRef) == "undefined" || window.needRef == "") {
        window.needRef = false;
    }
    if (window.needRef) {
        window.location.reload();
    } else {
        InitHeadTop();
        InitLogin();
    }
}


function ShowLogo(flashvars) {
    //var access = "<param name='allowNetworking' value='all'><param name='allowScriptAccess' value='always'>";
    //var network = " allowNetworking='all' AllowScriptAccess='always' ";
    var access = "<param name='allowNetworking' value='internal'><param name='allowScriptAccess' value='sameDomain'>";
    var network = " allowNetworking='internal' AllowScriptAccess='sameDomain' ";

    //域名更换 替换遗漏数据
    flashvars = flashvars.replace(/cdn.30edu.com\//ig, "cdn.30edu.com.cn/").replace(/logo1.30edu.com\//ig, "logo1.30edu.com.cn/").replace(/font.30edu.com\//ig, "font.30edu.com.cn/");
    flashvars = flashvars.replace(/\/\/cdn-portal-img.30edu.com.cn\//ig, "http://cdn-portal-img.30edu.com.cn/");
    flashvars = flashvars.replace(/\/\/cdn.30edu.com.cn\//ig, "http://cdn.30edu.com.cn/");
    document.writeln("<object id='logo' width='960' height='600' codebase='http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=10,0,0,0' classid='clsid:D27CDB6E-AE6D-11cf-96B8-444553540000'><param name='wmode' value='transparent'>" + access + "<param name='flashvars' value='" + flashvars + "'><param name='movie' value='//cdn.30edu.com.cn/Portal/Logo/LogoShow.swf'><param name='quality' value='high'><embed name='logo' width='960' height='600' flashvars='" + flashvars + "' " + network + " type='application/x-shockwave-flash' pluginspage='http://www.macromedia.com/go/getflashplayer' quality='high' wmode='transparent' src='//cdn.30edu.com.cn/Portal/Logo/LogoShow.swf'></object>");
}

