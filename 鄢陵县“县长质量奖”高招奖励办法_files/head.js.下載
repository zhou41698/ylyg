function UserDefectWin() {
    $.dialog({
        title: "完善信息",
        content: "<iframe src=\"https://www.30edu.com.cn/ifrUser.shtml\" scrolling=\"no\" allowtransparency=\"true\" frameborder=\"0\" width=\"860px\" height=\"380px\" id=\"UserDetailWin\"></iframe>",
        lock: true,
        cancel: true,
        cancelVal: "取消",
    });
}

function CheckDomain() {
    var str = "";
    $.ajax({
        async: false,
        type: "POST",
        url: "/Login.CheckDomain.data",
        data: { unitID: window.UnitID },
        dataType: "",
        success: function (data) {
            str = data;
        }
    })
    return str;
}

//长文本栏目宽度匹配
if ($(".menuBar").hasClass("long-text")) {
    //console.log("long-text:" + $(".menuBar").hasClass("long-text"));
    $(".head_menu .menu-ul").each(function () {
        //console.log(this);
        var flc = $(this).parent().width();
        var slc = $(this).outerWidth();
        var d = slc - $(this).width();
        //console.log("flc:", flc, "slc:", slc, "d:", d);
        if (flc >= slc) {
            $(this).css({ width: flc - d });
            $(this).children("li").css({ minWidth: flc - d });
        }
    })

    //console.log("evenline:" + $(".head_menu").hasClass("evenline"));
    if ($(".head_menu").hasClass("evenline")) {
        $(".head_menu .menu-ul-sub > ul").each(function () {
            //console.log(this);
            var slcli = $(this).parent().parent().width();
            var tlc = $(this).outerWidth();
            var d = tlc - $(this).width();
            //console.log("slcli:", slcli, "tlc:", tlc, "d:", d);
            if (slcli >= tlc) {
                $(this).css({ width: slcli - d });
            }
        })
    }
};